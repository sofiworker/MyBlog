<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.j2e.entities.TagBean" table="tag" schema="myblog">
        <id name="tagId">
            <column name="tag_id" sql-type="int(11)"/>
        </id>
        <property name="tagName">
            <column name="tag_name" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>