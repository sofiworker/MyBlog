<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.j2e.entities.TagEssayBean" table="tag_essay" schema="myblog">
        <composite-id mapped="true" class="com.j2e.entities.TagEssayBeanPK">
            <key-property name="tagId">
                <column name="tag_id" sql-type="int(11)"/>
            </key-property>
            <key-property name="essayId">
                <column name="essay_id" sql-type="char(32)" length="32"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>